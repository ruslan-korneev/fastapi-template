# Application
LOGGING_LEVEL=DEBUG
PROJECT_TITLE={{ cookiecutter.project_title }}
PROJECT_DESCRIPTION={{ cookiecutter.project_description }}
ENVIRONMENT=development

# Database
DB__HOST=localhost
DB__PORT=5432
DB__USERNAME=postgres
DB__PASSWORD=postgres
DB__NAME={{ cookiecutter.db_name }}

# Sentry (optional)
SENTRY__DSN=
SENTRY__ENABLE_TRACING=true
SENTRY__TRACES_SAMPLE_RATE=1.0
SENTRY__PROFILES_SAMPLE_RATE=1.0

# CORS Settings
CORS__ALLOW_ORIGINS=["*"]
CORS__ALLOW_CREDENTIALS=true
CORS__ALLOW_METHODS=["*"]
CORS__ALLOW_HEADERS=["*"]
CORS__MAX_AGE=600

# Rate Limiting
RATE_LIMIT__ENABLED=true
RATE_LIMIT__REQUESTS_PER_MINUTE=60
RATE_LIMIT__BURST_SIZE=10
